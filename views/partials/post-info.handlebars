<article class="post">
    <div class="title">
        <span>({{title}})</span>
        <span>City - ({{city_location}})</span>
    </div>
    <div class="meta">
        By {{user.username}} on {{created_at}}
        |
        <a href="/blog/{{id}}">{{comments.length}} comment(s)</a>
    </div>
</article>